#include <avr/io.h>       
#define F_CPU 16000000UL  
#include <util/delay.h>   

int main(void){

	DDRB |= (1<<PB3);          //making portb.3 output

	while(1){                 
		PORTB |= (1<<PB3);     //output "LED ON"
		_delay_ms(412.5);      //delay for 412ms @16MHz
		PORTB &= ~(1<<PB3);    //output "LED OFF"
		_delay_ms(337.5);      //delay for 337ms @16MHz
	}
	return 1;
} 
